{
  "name": "calm-go-pattern-search",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "wasm": "wasm-pack build ./rust/wasm-search --target web",
    "clippy": "cargo clippy --all-targets --all-features",
